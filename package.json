    "lodash.debounce": "^4.0.8"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.3.7",
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.5.0",
    "@rollup/plugin-image": "^3.0.3",
    "@types/jest": "^29.5.12",
    "@types/jsrsasign": "^10.5.14",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "antd": "^5.18.3",
