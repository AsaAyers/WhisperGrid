    "jest-environment-jsdom": "^29.7.0",
    "jotai": "^2.9.0",
    "parcel-bundler": "^1.12.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1"
  }
