{
  "name": "whispergrid",
  "version": "1.0.1",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
    "whispergrid": "bin.js"
  },
  "scripts": {
    "start": "parcel index.html",
    "cy:open": "cypress open",
    "cy:run": "cypress run -b electron --e2e",
    "precy:run": "npm run test",
    "test": "jest",
    "pretest": "npm run lint",
    "lint": "eslint src cypress",
    "prelint": "tsc --noEmit",
    "prebuild": "rm -rf dist",
    "build": "parcel build --no-minify index.html 404.html --public-url /WhisperGrid && parcel build src/cli/index.ts --target node -o cli",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "prepare": "husky"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^5.2.1",
    "jose": "^5.4.0",
